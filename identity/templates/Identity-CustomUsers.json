        "UserName" : "applicationadministrator",
        "Path" : "/",
        "Groups" : [
          { "Fn::ImportValue" : { "Fn::Sub" : "${CustomGroupsStackName}-ApplicationAdministratorsGroup" }}
        ]
      }
    },

    "WebSphereMQAdministratorUser" : {
