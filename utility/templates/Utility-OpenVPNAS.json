          { "Fn::If" : [ "ConfigureAdministratorNetwork7",  { "IpProtocol" : "icmp", "FromPort" :   "11", "ToPort" :   "11", "CidrIp" : { "Fn::Select" : [ "7", { "Fn::Split" : [ ",", { "Ref" : "AdministratorNetworks" }]}]}},  { "Ref" : "AWS::NoValue" }]},
          { "Fn::If" : [ "ConfigureAdministratorNetwork7",  { "IpProtocol" : "tcp",  "FromPort" :   "22", "ToPort" :   "22", "CidrIp" : { "Fn::Select" : [ "7", { "Fn::Split" : [ ",", { "Ref" : "AdministratorNetworks" }]}]}},  { "Ref" : "AWS::NoValue" }]},
          { "Fn::If" : [ "ConfigureAdministratorNetwork7",  { "IpProtocol" : "tcp",  "FromPort" :  "943", "ToPort" :  "943", "CidrIp" : { "Fn::Select" : [ "7", { "Fn::Split" : [ ",", { "Ref" : "AdministratorNetworks" }]}]}},  { "Ref" : "AWS::NoValue" }]},
          { "Fn::If" : [ "ConfigureAdministratorNetwork7",  { "IpProtocol" : "tcp",  "FromPort" :  "443", "ToPort" :  "443", "CidrIp" : { "Fn::Select" : [ "7", { "Fn::Split" : [ ",", { "Ref" : "AdministratorNetworks" }]}]}},  { "Ref" : "AWS::NoValue" }]},
          { "Fn::If" : [ "ConfigureAdministratorNetwork7",  { "IpProtocol" : "udp",  "FromPort" : "1194", "ToPort" : "1194", "CidrIp" : { "Fn::Select" : [ "7", { "Fn::Split" : [ ",", { "Ref" : "AdministratorNetworks" }]}]}},  { "Ref" : "AWS::NoValue" }]}
        ],
        "Tags" : [
          { "Key" : "Name", "Value" : { "Fn::Join" : [ "-", [ { "Fn::ImportValue" : { "Fn::Sub" : "${VPCStackName}-VPCName" }}, { "Ref" : "UtilityName" }, "InstanceSecurityGroup" ]]}}
        ]
      }
    },

    "DomainControllerSecurityGroupIngress3389" : {
      "Type" : "AWS::EC2::SecurityGroupIngress",
      "Properties" : {
        "GroupId" : { "Fn::ImportValue" : { "Fn::Sub" : "${ActiveDirectoryStackName}-DomainControllerSecurityGroup" }}, "IpProtocol" : "tcp", "FromPort" :  "3389", "ToPort" :  "3389", "SourceSecurityGroupId" : { "Ref" : "InstanceSecurityGroup" }
      },
      "Condition" : "ConfigureActiveDirectoryIntegration"
    },

    "GetHostName" : {
      "Type" : "Custom::GetHostName",
      "Properties" : {
        "ServiceToken" : { "Fn::ImportValue" : { "Fn::Sub" : "${CustomIdentifierFunctionsStackName}-GetHostNameFunctionArn" }},
        "CompanyName" : { "Ref" : "CompanyName" },
        "LocationName" : { "Ref" : "AWS::Region" },
        "EnvironmentName" : { "Fn::ImportValue" : { "Fn::Sub" : "${VPCStackName}-VPCName" }},
        "ApplicationName" : { "Ref" : "UtilityName" }
      }
    },

    "GetOpenVPNASImageId" : {
      "Type" : "Custom::GetOpenVPNASImageId",
      "Properties" : {
        "ServiceToken" : { "Fn::ImportValue" : { "Fn::Sub" : "${CustomAMIFunctionsStackName}-GetOpenVPNASImageIdFunctionArn" }},
        "Region" : { "Ref" : "AWS::Region" },
        "License" : { "Ref" : "UtilityLicense" },
        "Version" : { "Ref" : "UtilityVersion" }
      }
    },

    "Instance" : {
      "Type" : "AWS::EC2::Instance",
      "Properties" : {
        "ImageId" : { "Fn::GetAtt" : [ "GetOpenVPNASImageId", "ImageId" ]},
        "InstanceType" : { "Fn::FindInMap" : [ "InstanceTypeMap", "OpenVPNAS", { "Ref" : "EnvironmentType" }]},
        "KeyName" : { "Fn::If" : [ "ConfigureKey", { "Ref" : "KeyName" }, { "Ref" : "AWS::NoValue" }]},
        "NetworkInterfaces" : [{
          "Description"              : { "Fn::Join" : [ "-", [ { "Fn::ImportValue" : { "Fn::Sub" : "${VPCStackName}-VPCName" }}, { "Ref" : "UtilityName" }, "NetworkInterface", "eth0" ]]},
          "DeviceIndex"              : "0",
          "SubnetId"                 : { "Fn::ImportValue" : { "Fn::Sub" : "${VPCStackName}-Public1SubnetA" }},
          "GroupSet"                 : [{ "Ref" :  "InstanceSecurityGroup" }],
          "AssociatePublicIpAddress" : "false",
          "DeleteOnTermination"      : "true"
        }],
        "Tags" : [
          { "Key" : "Name", "Value" : { "Fn::Join" : [ "-", [ { "Fn::ImportValue" : { "Fn::Sub" : "${VPCStackName}-VPCName" }}, { "Ref" : "UtilityName" }, "Instance" ]]}},
          { "Key" : "HostName", "Value" : { "Fn::Sub" : "${GetHostName.HostName}01" }}
        ],
        "UserData" : { "Fn::Base64" : { "Fn::Join" : [ "\n", [
          { "Fn::Join" : [ "", [ "public_hostname=", { "Ref" : "UtilityDomain" }, ".", { "Fn::ImportValue" : { "Fn::Sub" : "${VPCStackName}-VPCPublicDomain" }}]]},
          "admin_user=openvpn",
          "admin_pw=changemeviaguiafterinstall",
          "reroute_gw=0",
          "reroute_dns=1"
        ]]}}
      }
    },

    "EIP" : {
      "Type" : "AWS::EC2::EIP",
      "Properties" : {
        "Domain" : "vpc"
      }
    },

    "EIPAssociation" : {
      "Type" : "AWS::EC2::EIPAssociation",
      "Properties" : {
        "AllocationId" : { "Fn::GetAtt" : [ "EIP", "AllocationId" ]},
        "InstanceId" : { "Ref" : "Instance" }
      }
    },

    "PublicRecordSet" : {
      "Type" : "AWS::Route53::RecordSet",
      "Properties" : {
        "HostedZoneId" : { "Fn::ImportValue" : { "Fn::Sub" : "${VPCStackName}-PublicHostedZone" }},
        "Comment" : { "Fn::Join" : [ "", [ "DNS name for ", { "Ref" : "UtilityName" }, " Instance" ]]},
        "Name" : { "Fn::Join" : [ "", [ { "Ref" : "UtilityDomain" }, ".", { "Fn::ImportValue" : { "Fn::Sub" : "${VPCStackName}-VPCPublicDomain" }}, "." ]]},
        "Type" : "A",
        "TTL" : "900",
        "ResourceRecords" : [{ "Ref" : "EIP" }]
      }
    }
  },

  "Outputs" : {
    "InstanceSecurityGroup" : {
      "Description" : "The Instance SecurityGroup",
      "Value" : { "Ref" : "InstanceSecurityGroup" },
      "Export" : { "Name" : { "Fn::Sub" : "${AWS::StackName}-InstanceSecurityGroup" }}
    },

    "EIP" : {
      "Description" : "The Public Address of the OpenVPN Instance",
      "Value" : { "Ref" : "EIP" },
      "Export" : { "Name" : { "Fn::Sub" : "${AWS::StackName}-EIP" }}
    },

    "PublicHostname" : {
      "Description" : "The Public Hostname of the OpenVPN Instance",
      "Value" : { "Fn::Join" : [ "", [ { "Ref" : "UtilityDomain" }, ".", { "Fn::ImportValue" : { "Fn::Sub" : "${VPCStackName}-VPCPublicDomain" }}]]},
      "Export" : { "Name" : { "Fn::Sub" : "${AWS::StackName}-PublicHostname" }}
    },

    "UserWebsiteURL" : {
      "Description" : "The User Website URL",
      "Value" : { "Fn::Join" : [ "", [ "https://", { "Ref" : "UtilityDomain" }, ".", { "Fn::ImportValue" : { "Fn::Sub" : "${VPCStackName}-VPCPublicDomain" }}, "/" ]]},
      "Export" : { "Name" : { "Fn::Sub" : "${AWS::StackName}-UserWebsiteURL" }}
    },

    "AdministratorWebsiteURL" : {
      "Description" : "The Administration Website URL",
      "Value" : { "Fn::Join" : [ "", [ "https://", { "Ref" : "UtilityDomain" }, ".", { "Fn::ImportValue" : { "Fn::Sub" : "${VPCStackName}-VPCPublicDomain" }}, ":943/admin/" ]]},
      "Export" : { "Name" : { "Fn::Sub" : "${AWS::StackName}-AdministratorWebsiteURL" }}
    }
  }
}
